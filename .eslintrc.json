{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["google", "plugin:prettier/recommended", "plugin:react/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "require-jsdoc": 0,
        "no-invalid-this": 0
    }
}
